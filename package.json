{
  "name": "markdown-api",
  "version": "0.0.1",
  "description": "A Node library to convert Markdown to usable javascript objects, just like you would with a REST request.",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run build && node dist/bundle.js",
    "build": "rollup src/markdownReader.js -o dist/bundle.js --f cjs",
    "test": "ava"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Ralph Broers",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-preset-es2015": "^6.24.1",
    "rollup": "^0.53.4"
  },
  "dependencies": {
    "rxjs": "^5.5.6"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/reefstah/markdown-api.git"
  },
  "keywords": [
    "markdown",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/reefstah/markdown-api/issues"
  },
  "homepage": "https://github.com/reefstah/markdown-api#readme"
}
